        <Video name="cpu.boost.multiple.instances"
               cluster="0,1,2"
               minFrequency="2000000,2600000,1108000" />
        <Video name="cpu.boost.720p180"
               cluster="0,1,2"
               minFrequency="2000000,2600000,2000000" />
        <Video name="c2.mtk.mpeg4.decoder"
               type="video/mp4v-es"
               driverIntf="v4l2"
               maxConcurrentInstances="32" />
