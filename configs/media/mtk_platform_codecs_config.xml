    <Encoders>
        <Video name="c2.mtk.h263.encoder"
               type="video/3gpp"
               driverIntf="v4l2"
               canSwapWidthHeight="true"
               maxConcurrentInstances="10" />
        <Video name="c2.mtk.mpeg4.encoder"
               type="video/mp4v-es"
               driverIntf="v4l2"
               canSwapWidthHeight="true"
